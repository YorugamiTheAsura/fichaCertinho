package entidades;

public class Moto extends Veiculos {

	private Integer cilindradas;

	public Moto(String placa, String marca, Integer ano, Double preco, Double precoTotal, String modelo,
			Integer cilindradas) {
		super(placa, marca, ano, preco, precoTotal, modelo);
		this.cilindradas = cilindradas;
	}

	public Integer getCilindradas() {
		return cilindradas;
	}

	public void setCilindradas(Integer cilindradas) {
		this.cilindradas = cilindradas;
	}

	@Override
	public String toString() {
		return " Moto - Placa :" + placa + " - marca : " + marca + ", - ano : " + ano + ", - preco bruto : " + preco
				+ ", - preco total : " + precoTotal;

	}

}
