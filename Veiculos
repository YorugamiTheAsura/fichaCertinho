package entidades;

public abstract class Veiculos {
	//classe abstrata, não existe tipo veiculo
	//porém todo veiculo tem esses atributos
	protected String placa;
	protected String marca;
	protected Integer ano;
	protected Double preco;
	protected Double precoTotal;
	protected String modelo;
	
	public Veiculos(String placa, String marca, Integer ano, Double preco,Double precoTotal,String modelo) {
		this.placa = placa;
		this.marca = marca;
		this.ano = ano;
		this.preco = preco;
		this.precoTotal = precoTotal;
		this.modelo= modelo;
	}

	


	// Getter e Setter 
	public String getModelo() {
	    return modelo;
	}

	public void setModelo(String modelo) {
	    this.modelo = modelo;
	}
	
	
	public void setPrecoTotal(Double precoTotal) {
		this.precoTotal = precoTotal;
	}
	
	public Double getPrecoTotal() {
		return precoTotal;
	}
	

	public String getPlaca() {
		return placa;
	}

	public void setPlaca(String placa) {
		this.placa = placa;
	}

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

	public Integer getAno() {
		return ano;
	}

	public void setAno(Integer ano) {
		this.ano = ano;
	}
	
	public void setPreco(Double preco) {
		this.preco = preco;
	}

	public Double getPreco() {
		return preco;
	}


	@Override
	public abstract String toString();
		
	
	
	
}
